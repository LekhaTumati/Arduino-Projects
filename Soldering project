int l1 = 8;
int l2 = 9;
int l3 = 10;
int l4 = 11;
int d = 200;

void setup()
{
pinMode(l1, OUTPUT);
pinMode(l2, OUTPUT);
pinMode(l3, OUTPUT);
pinMode(l4, OUTPUT);
}
void loop()
{

digitalWrite(l1, HIGH);
delay(d); 
digitalWrite(l1, LOW);
digitalWrite(l2, HIGH);
delay(d); 
digitalWrite(l2, LOW);
digitalWrite(l3, HIGH);
delay(d); 
digitalWrite(l3, LOW);
digitalWrite(l4, HIGH);
delay(d);  

digitalWrite(l4, LOW);
}
